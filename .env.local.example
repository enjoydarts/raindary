# Next.js
NEXTAUTH_URL=http://localhost:3000
NODE_ENV=development

# Auth.js
# 以下のコマンドで生成: openssl rand -hex 32
AUTH_SECRET=

# Raindrop.io OAuth
# https://raindrop.io/app/settings/integrations で取得
AUTH_RAINDROP_ID=
AUTH_RAINDROP_SECRET=

# Database (ローカル開発環境)
DATABASE_URL=postgresql://postgres:postgres@db:5432/raindary

# または Supabase Cloud を使用する場合
# SUPABASE_URL=
# SUPABASE_ANON_KEY=
# SUPABASE_SERVICE_ROLE_KEY=

# Inngest (ローカル開発環境)
INNGEST_EVENT_KEY=local_dev_key
INNGEST_SIGNING_KEY=local_signing_key
INNGEST_DEV=1
INNGEST_BASE_URL=http://inngest:8288

# Extract Service
EXTRACT_API_URL=http://extract:8000/extract

# Anthropic Claude
# https://console.anthropic.com/ で取得
ANTHROPIC_API_KEY=

# Token Encryption
# 以下のコマンドで生成: openssl rand -hex 32
ENCRYPTION_KEY=

# レート制限 (Upstash Redis - オプション、本番環境用)
# UPSTASH_REDIS_REST_URL=
# UPSTASH_REDIS_REST_TOKEN=
